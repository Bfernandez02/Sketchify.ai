{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
   "paths": {
      "@/*": [ "./*"],
      "@screens/*" : ["./screens/*"],
      "@assets/*": ["./assets/*"],
      "@types/*" : ["./types"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
